<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Phone</th>
    <th>Ispool</th>
    <th>Address</th>
    <th>Carengine</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.phone %></td>
    <td><%= user.ispool %></td>
    <td><%= user.address_id %></td>
    <td><%= user.carengine %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
